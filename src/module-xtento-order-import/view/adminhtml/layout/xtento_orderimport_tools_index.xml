<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\OrderImport\Block\Adminhtml\Widget\Menu" name="orderimport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\OrderImport\Block\Adminhtml\Tools" name="adminhtml.orderimport.tools">
                <block class="Xtento\OrderImport\Block\Adminhtml\Tools\Import" name="adminhtml.orderimport.tools.import" template="Xtento_OrderImport::tools/import.phtml"/>
                <block class="Xtento\OrderImport\Block\Adminhtml\Tools\Export" name="adminhtml.orderimport.tools.export" template="Xtento_OrderImport::tools/export.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>

