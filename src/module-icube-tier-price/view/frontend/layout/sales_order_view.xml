<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock class="Magento\Sales\Block\Order\Items" name="order_items" template="Icube_TierPrice::order/items.phtml"></referenceBlock>
        <referenceBlock name="order_totals">
            <block class="Icube\TierPrice\Block\Sales\Order\TierPrice" name="icubetierprice" template="Icube_TierPrice::order/tier_price.phtml" cacheable="false"/>
            <arguments>
                <argument name="label_properties" xsi:type="string">colspan="5" class="mark"</argument>
                <argument name="value_properties" xsi:type="string">class="amount"</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
